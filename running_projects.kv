<RunningProjects>:
    MDBoxLayout:
        orientation: 'vertical'
        size: root.width, root.height
        padding: 10
        spacing: 20

        MDFillRoundFlatButton:
            text: "Go Back"
            pos_hint: {'center_x': .5, 'center_y': .5}
            on_press:
                app.root.current = 'menu'
                root.manager.transition.direction = 'right'